{
    "manifest_version": 2,
    "name": "DCT",
    "description": "Digital Currency Trading",
    "version": "0.0.1",
	"background": {
        "scripts": ["js/libs/jquery.min.js", "js/background.js"]
	},
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "DCT(Digital Currency Trading)"
    },
    "permissions": [       
        "tabs",            
        "https://pet-chain.baidu.com/*",         
        "webRequestBlocking",
        "webRequest",
        "browsingData",
        "storage",
        "unlimitedStorage"
    ],
    "icons" : {
        "128": "images/icon.png"
    },
    "options_page":"options.html"
}
